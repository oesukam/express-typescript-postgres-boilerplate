{
  "name": "express-postgres-typescript",
  "version": "0.1.0",
  "description": "An Express Typescript Boilterplate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "build": "babel src --extensions .js,.ts --out-dir dist",
    "test": "NODE_ENV=test jest --watchAll --coverage --detectOpenHandles --forceExit",
    "test:ci": "NODE_ENV=test jest --coverage --detectOpenHandles --forceExit",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "format:check": "prettier --ignore-path .gitignore --list-different \"**/*.+(ts|js|json)\"",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "db:create": "npm run sequelize db:create",
    "db:drop": "npm run sequelize db:drop",
    "db:migrate": "npm run sequelize db:migrate",
    "db:migrate:reset": "npm run sequelize db:migrate:undo:all",
    "db:seed": "npm run sequelize db:seed:all",
    "db:migrate:seed": "npm run sequelize db:migrate && npm run db:seed",
    "pm2:start": "pm2 start app.json",
    "pm2:deploy": "npm run build && npm run pm2:start",
    "pm2:stop": "pm2 stop app.json",
    "pm2:destroy": "pm2 delete app.json",
    "pm2:logs": "pm2 logs api"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oesukam/express-typescript-postgres-boilerplate.git"
  },
  "keywords": [
    "express",
    "typescript",
    "node",
    "postgres",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "git@github.com:oesukam/express-typescript-postgres-boilerplate/issues"
  },
  "homepage": "git@github.com:oesukam/express-typescript-postgres-boilerplate#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.2.0",
    "@sentry/node": "^5.17.0",
    "@types/minimist": "^1.2.1",
    "app-root-path": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "event-emitter": "^0.3.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "faker": "^4.1.0",
    "firebase-admin": "^9.4.2",
    "ip2location-nodejs": "^8.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "make-runnable": "^1.3.8",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "querystring": "^0.2.0",
    "request-ip": "^2.1.3",
    "sequelize": "^5.21.13",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.9.5",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/app-root-path": "^1.2.4",
    "@types/async": "^3.2.4",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/event-emitter": "^0.3.3",
    "@types/express": "^4.17.6",
    "@types/express-useragent": "^1.0.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/request-ip": "^0.0.35",
    "@types/speakeasy": "^2.0.5",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.1",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-watch": "^7.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-json": "^2.1.1",
    "jest": "^26.0.1",
